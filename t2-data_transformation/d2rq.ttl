# D2RQ Namespace
@prefix d2rq: <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#> .
# Namespace of the ontology
@prefix : <http://www.semanticweb.org/sssw/ontologies/2016/6/synset#> .

# Namespace of the mapping file; does not appear in mapped data
@prefix map: <file:///home/sssw/sssw16-hack/t2-data_transformation/ontology.ttl#> .
# Other namespaces
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
map:Database a d2rq:Database;
    d2rq:jdbcDSN "jdbc:mysql://localhost:3306/SentiWordNet";
    d2rq:jdbcDriver "com.mysql.jdbc.Driver";
    d2rq:username "root";
    d2rq:password "bertinoro";
    .
# CREATE individuals of swn:Synset from the table synset of
#the DB SentiWordNet
# The URI is generated by using
# the pattern http://www.example.org/swn/resource/SYNSET.ID
map:SentiWordNet a d2rq:ClassMap;
    d2rq:dataStorage map:Database;
    d2rq:class :SentiWord;
    d2rq:uriPattern "http://www.example.org/swn/resource/@@synset.ID@@" .


map:wordPOS a d2rq:PropertyBridge;
    d2rq:belongsToClassMap map:SentiWordNet;
    d2rq:property :pos;
    d2rq:column "synset.POS";
    d2rq:lang "en";
    d2rq:propertyDefinitionLabel "position"@en;


map:positiveScore a d2rq:PropertyBridge;
    d2rq:belongsToClassMap map:SentiWordNet;
    d2rq:property :positiveScore;
    d2rq:column "synset.PosScore";
    d2rq:propertyDefinitionLabel "score"@en;

map:positiveScore a d2rq:PropertyBridge;
    d2rq:belongsToClassMap map:SentiWordNet;
    d2rq:property :negativeScore;
    d2rq:column "synset.NegScore";
    d2rq:propertyDefinitionLabel "score"@en;

map:synsetTerms a d2rq:PropertyBridge;
    d2rq:belongsToClassMap map:SentiWordNet;
    d2rq:property :terms;
    d2rq:column "synset.SynsetTerms";
    d2rq:propertyDefinitionLabel "terms"@en;

map:glossary a d2rq:PropertyBridge;
    d2rq:belongsToClassMap map:SentiWordNet;
    d2rq:property :glossary;
    d2rq:column "synset.Gloss";
    d2rq:propertyDefinitionLabel "glossary"@en;
